/* FIXED THD BOOK */
$(function(){
	var sbOffLeft = $('#sidebar').offset().left+11;
	
	$(window).resize(function(){
		sbOffLeft = $('#sidebar').offset().left+11;
		if('fixed' != $('.nav').css('position')){
			$('.nav').css('left',sbOffLeft-$(window).scrollLeft());
		}
		if($(window).scrollTop()>963){
			$('.nav').addClass("fixed-book")
            $('.nav').css('position', 'fixed');
            $('.nav').css('top', 49);
			$('.nav').css('left',sbOffLeft-$(window).scrollLeft());
		}
		})
	$(window).scroll(function(e){
        if($(window).scrollTop()>963){
			$('.nav').addClass("fadeInDownBig animated")
            $('.nav').css('position', 'fixed');
            $('.nav').css('top', 49);
			$('.nav').css('left',sbOffLeft-$(window).scrollLeft());
			
        } else {
			$('.nav').removeClass("fadeInDownBig animated")
            $('.nav').css('position', '');
            $('.nav').css('top', '');
			$('.nav').css('left', '');
        }
		if(0 < $(window).scrollTop()){
	    	$('.top').fadeIn();
		}else{
	   		$('.top').fadeOut();
		}
		return false;
    });
	// scroll body to 0px on click
		$('.top').click(function () {
		$('body,html').animate({
			scrollTop: 0
		}, 800);
		return false;
	});
	})

/* archive scroll */
$(function(){
		$(".scroll").click(function(event){		
			event.preventDefault();
			$('html,body').animate({scrollTop:$(this.hash).offset().top}, 500);
		});
	});


/* Slides JS */
$(function(){
	$('#slides').slides({
		preload: true,
		preloadImage: 'images/loading.gif',
		play: 3000,
		pause: 100,
		hoverPause: true
	});
});

